/* pages/me/me.wxss */
page{
  background: #fff;
}
.backWall{
  background: #ffdfbd;
  width: 100%;
  height: 200rpx; 
}
.userinfo-avatar {
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 50%;
  position: absolute;
  top: 80rpx;
  left: 50rpx;
  box-shadow: 0 0 0 1px #eee,
    0 1px 2px 1px #efefef;
}
.userinfo-iavatar {
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 50%;
  position: absolute;
  top: 80rpx;
  right: 50rpx; 
  box-shadow: 0 0 0 1px #eee,
    0 1px 2px 1px #efefef;
}
.me_list{
  width: 92%;
  margin: 20rpx auto;
  background: #fff;
  box-shadow: 0 0 0 1px #ffc59b,
    0 1px 2px 1px #efefef;
  border: 1px solid #fff;
  border-radius: 8rpx;
  position: relative;
  top: 200rpx;
}
.line{
  height: 1px;
  width: 100%;
  margin: 0 auto;
  background: #ffc59b;
}